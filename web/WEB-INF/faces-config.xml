<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundle2</base-name>
            <var>bundle2</var>
        </resource-bundle>
    </application>
    <application>
        <el-resolver>beans.util.JsfCrudELResolver</el-resolver>
    </application>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>welcome</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>efetivo</managed-bean-name>
        <managed-bean-class>beans.EfetivoController</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>efetivoJpa</managed-bean-name>
        <managed-bean-class>facade.EfetivoJpaController</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <converter>
        <converter-for-class>model.Efetivo</converter-for-class>
        <converter-class>beans.EfetivoConverter</converter-class>
    </converter>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>efetivo_create</from-outcome>
            <to-view-id>/efetivo/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>efetivo_list</from-outcome>
            <to-view-id>/efetivo/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>efetivo_edit</from-outcome>
            <to-view-id>/efetivo/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>efetivo_detail</from-outcome>
            <to-view-id>/efetivo/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>funcao</managed-bean-name>
        <managed-bean-class>beans.FuncaoController</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>funcaoJpa</managed-bean-name>
        <managed-bean-class>facade.FuncaoJpaController</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <converter>
        <converter-for-class>model.Funcao</converter-for-class>
        <converter-class>beans.FuncaoConverter</converter-class>
    </converter>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>funcao_create</from-outcome>
            <to-view-id>/funcao/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>funcao_list</from-outcome>
            <to-view-id>/funcao/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>funcao_edit</from-outcome>
            <to-view-id>/funcao/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>funcao_detail</from-outcome>
            <to-view-id>/funcao/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
